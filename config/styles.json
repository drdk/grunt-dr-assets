{
  "bootstrap-mixins": {
    "cwd": "/",
    "compile": false,
    "files": [
      "mixins/*.less",
      "mixins.less"
    ],
    "dest": "less/bootstrap/"
  },
  "bootstrap-components": {
    "cwd": "/",
    "compile": {
      "dest": "css/dr-global-components/bootstrap"
    },
    "files": [
    ],
    "dest": "less/bootstrap/components/"
  },
  "bootstrap-core": {
    "cwd": "/",
    "compile": false,
    "files": [
      "mixins/*.less",
      "mixins.less",
      "normalize.less",
      "print.less",
      "scaffolding.less",
      "type.less",
      "grid.less",
      "navs.less",
      "navbar.less",
      "labels.less",
      "responsive-embed.less",
      "utilities.less",
      "responsive-utilities.less"
    ],
    "dest": "less/bootstrap/"
  },
  "dr-mixins": {
    "cwd": "/",
    "compile": false,
    "files": [
      "mixins/*.less", 
      "mixins.less", 
      "variables.less"
    ],
    "dest": "less/dr/"
  },
  "dr-components": {
    "cwd": "/components/",
    "compile": {
      "dest": "css/dr-global-components/dr"
    },
    "files": "**/*.less",
    "dest": "less/dr/components/"
  },
  "dr-core": {
    "cwd": "/",
    "compile": {
      "dest": "css/dr-global-core.css"
    },
    "files": [
      "mixins/*.less",
      "*.less",
      "!dr-include.less"
    ],
    "dest": "less/dr/"
  },
  "dr-build": {
    "cwd": "/",
    "compile": false,
    "files": [
      "dr-include.less",
      ".csscomb.json",
      ".csslintrc"
    ],
    "dest": "less/"
  }
}